{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "strictPropertyInitialization": false
  },
  "include": [
    "src/**/*"
, "src/build"  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}