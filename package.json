{
  "name": "orchestx-server",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "build": "tsc --project . && npm run copy-build && npm run copy-qr-build",
    "serve": "node dist/app.js",
    "dev": "nodemon src/app.ts --exec ts-node src/app.ts",
    "clean": "rimraf dist",
    "copy-build": "xcopy /E /I /Y src\\build dist\\build",
    "copy-qr-build": "if exist src\\qr-build xcopy /E /I /Y src\\qr-build dist\\qr-build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "directory-tree": "^3.5.1",
    "express": "^4.19.2",
    "net": "^1.0.2",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.7",
    "@types/ws": "^8.5.14",
    "nodemon": "^3.1.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
